    {% extends '@Admin/base.html.twig' %}
{% block navegacion %}
{{ include('@Admin/partials/navegacion.html.twig',{'menu':'Compuglobal','submenu':'Inscrito'}) }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}

{% endblock %}

{% block javascripts %}
    {{ parent() }}
<script src="{{ asset('admin/js/jasny-bootstrap.js') }}"></script>
{% endblock %}

    {% block body %}
    <div class="row">
        <div class="col-md-12">
            <div class="white-box">
                <h3 class="box-title m-b-0">Ver Registro</h3>                <form class="form-horizontal" role="form">                        <div class="form-group">
                            <label class="col-sm-2 control-label">Id</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ inscripciones.id }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                        <label class="col-sm-2 control-label">Curso</label>
                        <div class="col-sm-10">
                            <p class="form-control-static">                                        <a href="{{ path('curso_show',{id:inscripciones.curso.id}) }}">{{ inscripciones.curso }}</a>                                </p>
                        </div>
                        </div>                        <div class="form-group">
                        <label class="col-sm-2 control-label">Nombre</label>
                        <div class="col-sm-10">
                            <p class="form-control-static">                                        {{ inscripciones.nombre }}                                </p>
                        </div>
                        </div>                        <div class="form-group">
                            <label class="col-sm-2 control-label">Email</label>
                            <div class="col-sm-10">
                                <p class="form-control-static">                                        {{ inscripciones.email }}                                </p>
                            </div>
                        </div>                        <div class="form-group">
                        <label class="col-sm-2 control-label">Telefono</label>
                        <div class="col-sm-10">
                            <p class="form-control-static">                                        {{  inscripciones.telefono }}                                </p>
                        </div>
                        </div>                        <div class="form-group m-b-0">
                        <div class="col-sm-12">
                                                                    <a class="btn btn-inverse waves-effect waves-light m-t-10" style="float:left; margin:5px;" href="{{ path('inscripciones_index') }}">Regresar</a>           </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    {% endblock %}
